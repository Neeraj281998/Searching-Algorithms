def BinarySearch(arr,left,right,target):
    if right>=left:
        mid=(left+right)//2
        if (target==arr[mid]):
            return mid
        if(target<arr[mid]):
            return BinarySearch(arr,left,mid-1,target)
        else:
            return BinarySearch(arr,mid+1,right,target)
    else:
        return -1
arr=[-1,0,1,2,3,4,7,9,10,20]
print(BinarySearch(arr,0,len(arr)-1,10))
